const langMenu = document.querySelector(".js-langMenu");
const burgerMenu = document.querySelector(".js-burgerMenu");

langMenu.addEventListener("click", () => {
    const langSubmenu = langMenu.querySelector(".js-langSubmenu");
    langSubmenu.classList.toggle("lang-submenu--show");
});

burgerMenu.addEventListener("click", () => {
    const contactsBlock = document.querySelector(".js-contacts");
    contactsBlock.classList.toggle("contacts--show");
})